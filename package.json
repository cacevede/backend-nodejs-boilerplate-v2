{
  "name": "boilerplate-node",
  "version": "1.0.0",
  "description": "A Backend Node.js Boilerplate for Lean Tech Projects",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "dev": "set DEBUG=app:* && nodemon server",
    "start": "set NODE_ENV=production && node server",
    "test": "mocha --exit",
    "cover": "nyc npm run test",
    "report": "nyc report --reporter=html && open coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cacevede/backend-nodejs-boilerplate-v2"
  },
  "author": "Lean Tech IO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cacevede/backend-nodejs-boilerplate-v2/issues"
  },
  "homepage": "https://github.com/cacevede/backend-nodejs-boilerplate-v2#readme",
  "devDependencies": {
    "chalk": "^2.4.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.3",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "proxyquire": "^2.1.3",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "routes",
      "services",
      "lib",
      "utils"
    ]
  },
  "dependencies": {
    "@hapi/boom": "^8.0.1",
    "bcrypt": "^3.0.7",
    "cors": "^2.8.5",
    "date-fns": "^2.8.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.8.1",
    "morgan": "^1.9.1",
    "mysql2": "^2.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.17.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "winston": "^3.2.1"
  }
}
